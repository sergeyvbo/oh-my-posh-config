{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "green": "#98C379",
    "cyan": "#56B6C2",
    "blue": "#5FAAE8",
    "red": "#BF616A",
    "pink": "#D0666F",
    "yellow": "#E5C07B"
  },
  "secondary_prompt": {
    "template": "➜   ",
    "foreground": "p:green"
  },
  "transient_prompt": {
    "template": "➜  <p:cyan>{{ .PWD }}></> ",
    "foreground": "p:green"
  },
  "upgrade": {
    "source": "cdn",
    "interval": "168h",
    "auto": false,
    "notice": false
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "p:green",
          "properties": {
            "threshold": 500,
            "style": "austin",
            "always_enabled": true
          }
        },
        {
          "template": "➜",
          "foreground": "p:green",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "style": "folder"
          },
          "template": "  {{ .PWD }}",
          "foreground": "p:cyan",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " <p:blue>(</>{{ .UpstreamIcon }}  {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <p:yellow> {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} <p:green> {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }}<p:blue>)</>",
          "foreground": "p:pink",
          "type": "git",
          "style": "plain"
        },
        {
          "template": " ✗",
          "foreground": "p:red",
          "type": "status",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
